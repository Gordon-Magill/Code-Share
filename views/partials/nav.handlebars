<!--Desktop Nav Menu-->
<nav id="menu"
  class="bg-gray px-2 sm:px-4 py-2.5 dark:bg-gray-900 fixed z-20 top-0 left-0 border-b border-gray-200 dark:border-gray-600 w-screen">
  <div class="flex flex-wrap justify-between items-center mx-auto">
  <a href="/" class="flex items-center">
    <img src="/assets/CodeShareIcon.svg" class="mr-3 h-6 sm:h-9 mainLogo" alt="CodeShare Logo">
    <span class="self-center text-xl font-semibold whitespace-nowrap text-white">Git<span class="text-alt-blue/70">t</span><span class="text-alt-pink/70">y</span>Up</span>
  </a>
  <div class="flex md:order-2">
    {{#if session.loggedIn}}
    <p class="hidden sm:block self-center text-sm font-semibold whitespace-nowrap text-white px-4">Hi, {{session.username}}
    </p>
    <button id="logout" type="button"
        class="text-gray bg-blue hover:bg-gray-light hover:text-white font-medium rounded-lg text-sm px-5 py-2.5 text-center md:mr-2"><a href="/logout">Log
        Out</a></button>
    {{else}}
      <button id="login" type="button"
        class="text-gray bg-blue hover:bg-gray-light hover:text-white font-medium rounded-lg text-sm px-5 py-2.5 text-center  md:mr-2"><a href="/login">Log
        In</a></button>
    {{/if}}
    <button onClick="document.getElementById('hamMenu').classList.toggle('hidden')" type="button"
      class="inline-flex items-center p-2 text-sm rounded-lg md:hidden text-white"
      aria-controls="navbar-sticky" aria-expanded="false">
      <span class="sr-only">Open main menu</span>
      <svg class="w-6 h-6" aria-hidden="true" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg">
        <path fill-rule="evenodd"
          d="M3 5a1 1 0 011-1h12a1 1 0 110 2H4a1 1 0 01-1-1zM3 10a1 1 0 011-1h12a1 1 0 110 2H4a1 1 0 01-1-1zM3 15a1 1 0 011-1h12a1 1 0 110 2H4a1 1 0 01-1-1z"
          clip-rule="evenodd"></path>
      </svg>
    </button>
  </div>
  <div class="transition-all hidden justify-between items-center w-full md:flex md:w-auto md:order-1" id="navbar-sticky">
    <ul
      class="flex flex-col p-4 mt-4 bg-gray-50 rounded-lg border border-gray-100 md:flex-row md:space-x-8 md:mt-0 md:text-sm md:font-medium md:border-0">
      <li>
        <a href="/" class="hover:text-blue block py-2 pr-4 pl-3 text-white rounded md:bg-transparent md:p-0">Home</a>
        </li>
        <li>
        <a href="/about" class="hover:text-blue block py-2 pr-4 pl-3 text-white rounded md:bg-transparent md:p-0">About</a>
        </li>
        <li>
        <a href="/dashboard" class="hover:text-blue block py-2 pr-4 pl-3 text-white rounded md:bg-transparent md:p-0">Dashboard</a>
        </li>
      </ul>
  </div>
</div>
<!--Mobile Hamburger Menu-->
<div id="hamMenu" class="">
  <ul
    class="flex flex-col p-4 mt-4 bg-gray-50 rounded-lg border border-gray-100 md:flex-row md:space-x-8 md:mt-0 md:text-sm md:font-medium md:border-0">
    <li>
      <a href="/" class="hover:text-blue block py-2 pr-4 pl-3 text-white">Home</a>
    </li>
    <li>
      <a href="/about" class="hover:text-blue block py-2 pr-4 pl-3 text-white">About</a>
    </li>
    <li>
      <a href="/dashboard" class="hover:text-blue block py-2 pr-4 pl-3 text-white">Dashboard</a>
    </li>
  </ul>
</div>
</nav>
